{
  "name": "snz-theme",
  "publisher": "Snz",
  "displayName": "Snz's Theme",
  "description": "Snz's customized theme",
  "icon": "icon.png",
  "license": "MIT",
  "maintainers": [
    "Jiaxin Guo (郭佳鑫)"
  ],
  "contributors": [
    "Jiaxin Guo (郭佳鑫)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SnzFor16Min/snz-theme.git"
  },
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "dark",
    "theme",
    "color-theme",
    "custom",
    "snz",
    "go",
    "python",
    "c++"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Snz's Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Snz-Theme-color-theme.json"
      }
    ],
    "configurationDefaults": {
      "editor.semanticHighlighting.enabled": true,
      "go.useLanguageServer": true,
      "gopls": {
        "ui.semanticTokens": true
      },
      "go.diagnostic.vulncheck": "Imports"
    }
  }
}
