{
  "name": "Snz's Theme",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#2b2b2b",
    "editor.background": "#2b2b2b",
    "editorCursor.foreground": "#55a5ff",
    "editorHoverWidget.background": "#2b2b2b",
    "editorHoverWidget.border": "#3f3f3f",
    "editorInlayHint.background": "#393939",
    "editorInlayHint.foreground": "#787878",
    "editorInlayHint.typeForeground": "#49d8bb44",
    "editorLineNumber.foreground": "#555555",
    "editorLineNumber.activeForeground": "#999999",
    "editorGutter.foldingControlForeground": "#6f737a",
    "editor.lineHighlightBackground": "#333333",
    "errorForeground": "#ff5c7c",
    "menu.background": "#2b2b2b",
    "sideBar.background": "#2b2b2b",
    "statusBar.background": "#2b2b2b",
    "textLink.foreground": "#61afe1",
    "titleBar.activeBackground": "#2b2b2b",
    "titleBar.inactiveBackground": "#2b2b2b",
    "editorBracketHighlight.foreground1": "#8BD66E",
    "editorBracketHighlight.foreground2": "#E6954E",
    "editorBracketHighlight.foreground3": "#e0e0e0",
    "editorBracketHighlight.foreground4": "#49C7A2",
    "editorBracketHighlight.foreground5": "#C28F5C",
    "editorBracketHighlight.foreground6": "#7EC59A",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ff5c7c"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "property": "#a0e1ff",
    "parameter": {
      //   "foreground": "#bbbbbb",
      "underline": true
    },
    "selfParameter": {
      "foreground": "#94558d",
      "underline": false
    },
    // "class": "#49d8bb",
    // "class.builtin": "#4d9fe3",
    "type": "#49d8bb",
    "type.defaultLibrary": "#4d9fe3",
    "typeParameter": "#94558d",
    "namespace": {
      "foreground": "#ff9696",
      "italic": true
    },
    // "": {
    //   "foreground": "#ff0000",
    //   "italic": true
    // },
    "module": {
      "foreground": "#ff9696",
      "italic": true
    },
    // "keyword": "#c88cdc",
    // "function": "#e4e0ac",
    "function.declaration": "#ffc86e",
    "function.definition": "#ffc86e",
    "function.builtin": "#4d9fe3",
    "function.defaultLibrary": "#c88cdc",
    // "method": "#e4e0ac",
    "method.declaration": "#ffc86e",
    "method.definition": "#ffc86e",
    // "builtinConstant": "#c88cdc",
    "enumMember": "#4ec6ff",
    // "variable": "#bbbbbb",
    "variable.readonly": "#4ec6ff",
    "variable.defaultLibrary": "#4d9fe3",
    "string.format": "#4ec6ff"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#6a9955"
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#49d8bb"
      }
    },
    {
      "scope": ["storage.type.string", "constant.language", "storage.type.built-in", "storage.type.numeric"],
      "settings": {
        "foreground": "#4d9fe3"
      }
    },
    {
      "scope": [
        "string.quoted",
        "string.unquoted",
        "string.regexp.quoted",
        "support.type.property-name",
        "string.regexp.character-class"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    // {
    //   "scope": ["entity.name.function"],
    //   "settings": {
    //     "foreground": "#ffc86e",
    //   }
    // },
    {
      "scope": [
        "meta.function-call.generic",
        "entity.name.function.support",
        "entity.name.function.call",
        "entity.name.function.member",
        "support.function",
        "keyword.operator.negation.regexp"
      ],
      "settings": {
        "foreground": "#e4e0ac"
      }
    },
    {
      "scope": [
        "keyword.operator.comparison",
        "keyword.operator.assignment.python",
        "keyword.operator.arithmetic",
        "keyword.operator.logical.go",
        "keyword.operator.logical.cpp",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.bitwise",
        "punctuation.separator.pointer-access",
        "keyword.operator.pointer",
        "keyword.operator.shell",
        "punctuation.separator.statement",
        "keyword.operator.pipe",
        "keyword.operator.logical.shell",
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.operator.logical.python",
        "entity.name.function.support.builtin",
        "keyword.package",
        "keyword.type",
        "keyword.struct",
        "keyword.function",
        "keyword.interface",
        "keyword.map",
        "keyword.const",
        "keyword.var",
        "keyword.other",
        "storage.modifier",
        "keyword.operator.new",
        "storage.type.class",
        "storage.type.modifier",
        "keyword.operator.delete",
        "keyword.operator.sizeof",
        "keyword.operator.cast",
        "storage.type.struct",
        "storage.type.template",
        "keyword.operator.noexcept",
        "storage.type.function",
        "keyword.operator.ellipsis",
        "keyword.operator.redirect",
        "storage.type.string",
        "support.function.builtin",
        "keyword.operator.unpacking",
        "keyword.operator.address",
        "punctuation.definition.range",
      ],
      "settings": {
        "foreground": "#C88CDC"
      }
    },
    {
      "scope": [
        // "constant",
        "variable.upper-case",
        "entity.name.other.preprocessor.macro.predefined",
        "entity.other.attribute-name.pragma.preprocessor",
        "support.variable.magic",
        "constant.other.placeholder",
        "punctuation.definition.list",
        "punctuation.definition.block.sequence.item",
        "keyword.operator.quantifier.regexp",
        "keyword.operator.disjunction.regexp",
        "support.other.parenthesis",
        "keyword.operator.pattern.case.default.shell",
        "punctuation.separator.regex.alternation",
        "punctuation.definition.regex.arbitrary-repetition",
        "keyword.operator.redirect.stderr",
        "keyword.operator.redirect.stdout"
      ],
      "settings": {
        "foreground": "#4ec6ff"
      }
    },
    {
      "scope": ["markup.heading", "punctuation.character.set", "punctuation.definition.character-class"],
      "settings": {
        "foreground": "#4ec6ff",
        "fontStyle": "bold"
      }
    },
    // {
    //   "scope": "variable.parameter.function-call",
    //   "settings": {
    //     "fontStyle": ""
    //   }
    // },
    // {
    //   "scope": "variable.parameter",
    //   "settings": {
    //     "fontStyle": "underline"
    //   }
    // },
    // {
    //   "scope": "storage.type",
    //   "settings": {
    //     "foreground": "#4d9fe3"
    //   }
    // },
    {
      "scope": ["variable.other.import", "entity.name.namespace", "entity.name.scope-resolution"],
      "settings": {
        "foreground": "#ff9696",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.character.escape",
        "constant.other.option",
        "entity.name.tag",
        "support.type.property-name",
        "variable.parameter.function-call",
        "variable.parameter.function",
      ],
      "settings": {
        "foreground": "#ff9696"
      }
    },
    {
      "scope": [
        "constant.numeric.decimal",
        "constant.numeric.exponent",
        "constant.numeric.hexadecimal",
        "constant.numeric.integer",
        "constant.numeric.json",
        "constant.numeric.dec",
        "meta.diff.index.git",
        "meta.toc-list.line-number",
      ],
      "settings": {
        "foreground": "#b8dbb4"
      }
    },
    {
      "scope": ["variable.other.property", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#a0e1ff"
      }
    },
    {
      "scope": [
        "entity.name.function.definition",
        "punctuation.definition.subshell",
        "punctuation.definition.evaluation",
        "entity.name.function.shell",
        "keyword.operator.expansion.shell",
        "punctuation.separator.statement.background",
        "meta.diff.header.git",
      ],
      "settings": {
        "foreground": "#ffc86e"
      }
    },
    {
      "scope": [
        "meta.preprocessor.macro",
        "variable.language.this",
        "entity.name.function.preprocessor",
        "variable.other.dockerfile",
        "variable.other.normal.shell",
        "variable.language.special.shell",
        "punctuation.definition.variable.shell",
        "variable.parameter.positional",
        "support.other.escape.special.regexp"
      ],
      "settings": {
        "foreground": "#94558d"
      }
    },
    {
      "scope": ["constant.character.escape.line-continuation"],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "scope": ["support.other.match"],
      "settings": {
        "foreground": "#839496"
      }
    },
    {
      "scope": ["markup.deleted", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#ff5c7c"
      }
    },
    {
      "scope": ["markup.inserted", "markup.changed", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#96BE78"
      }
    },
  ]
}
