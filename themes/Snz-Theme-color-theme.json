{
  "name": "Snz's Theme",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#2b2b2b",
    "editor.background": "#2b2b2b",
    "errorForeground": "#ff5c7c",
    "menu.background": "#2b2b2b",
    "sideBar.background": "#2b2b2b",
    "statusBar.background": "#2b2b2b",
    "textLink.foreground": "#61afe1",
    "titleBar.activeBackground": "#2b2b2b",
    "titleBar.inactiveBackground": "#2b2b2b"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "property": "#a0e1ff",
    "parameter": {
      //   "foreground": "#bbbbbb",
      "underline": true
    },
    "selfParameter": {
      "foreground": "#94558d",
      "underline": false
    },
    // "class": "#0fc799",
    // "class.builtin": "#4d9fe3",
    "type": "#0fc799",
    "namespace": {
      "foreground": "#ff9696",
      "italic": true
    },
    // "": {
    //   "foreground": "#ff0000",
    //   "italic": true
    // },
    "module": {
      "foreground": "#ff9696",
      "italic": true
    },
    // "keyword": "#c88cdc",
    // "function": "#e4e0ac",
    "function.declaration": "#ffc86e",
    "function.builtin": "#4d9fe3",
    // "method": "#e4e0ac",
    // "method.declaration": "#ffc86e",
    // "builtinConstant": "#c88cdc",
    "enumMember": "#4ec6ff",
    // "variable": "#bbbbbb",
    "variable.defaultLibrary": "#4d9fe3"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#6a9955"
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#0fc799"
      }
    },
    {
      "scope": [
        "storage.type.string",
        "support.function.builtin",
        "constant.language",
        "storage.type.built-in",
        "storage.type.numeric"
      ],
      "settings": {
        "foreground": "#4d9fe3"
      }
    },
    {
      "scope": ["string", "support.type.property-name"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    // {
    //   "scope": ["entity.name.function"],
    //   "settings": {
    //     "foreground": "#ffc86e",
    //   }
    // },
    {
      "scope": [
        "meta.function-call.generic",
        "entity.name.function.support",
        "entity.name.function.call",
        "entity.name.function.member",
        "support.function"
      ],
      "settings": {
        "foreground": "#e4e0ac"
      }
    },
    {
      "scope": [
        "keyword.operator.comparison",
        "keyword.operator.assignment.python",
        "keyword.operator.arithmetic",
        "keyword.operator.logical.go",
        "keyword.operator.logical.cpp",
        "keyword.operator.address",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.bitwise",
        "punctuation.separator.pointer-access",
        "keyword.operator.pointer"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.operator.logical.python",
        "entity.name.function.support.builtin",
        "keyword.package",
        "keyword.type",
        "keyword.struct",
        "keyword.function",
        "keyword.interface",
        "keyword.map",
        "keyword.const",
        "keyword.var",
        "keyword.other",
        "storage.modifier",
        "keyword.operator.new",
        "storage.type.class",
        "storage.type.modifier",
        "keyword.operator.delete",
        "keyword.operator.sizeof",
        "keyword.operator.cast",
        "storage.type.struct",
        "storage.type.template",
        "keyword.operator.noexcept",
        "storage.type.function.python"
      ],
      "settings": {
        "foreground": "#C88CDC"
      }
    },
    {
      "scope": [
        "constant",
        "variable.upper-case",
        "entity.name.other.preprocessor.macro.predefined",
        "entity.other.attribute-name.pragma.preprocessor",
        "support.variable.magic"
      ],
      "settings": {
        "foreground": "#4ec6ff"
      }
    },
    // {
    //   "scope": "variable.parameter.function-call",
    //   "settings": {
    //     "fontStyle": ""
    //   }
    // },
    {
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "underline"
      }
    },
    // {
    //   "scope": "storage.type",
    //   "settings": {
    //     "foreground": "#4d9fe3"
    //   }
    // },
    {
      "scope": ["variable.other.import", "entity.name.namespace", "entity.name.scope-resolution"],
      "settings": {
        "foreground": "#ff9696",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b8dbb4"
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#a0e1ff"
      }
    },
    {
      "scope": ["entity.name.function.definition"],
      "settings": {
        "foreground": "#ffc86e"
      }
    },
    {
      "scope": ["meta.preprocessor.macro", "variable.language.this", "entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#94558d"
      }
    }
  ]
}
